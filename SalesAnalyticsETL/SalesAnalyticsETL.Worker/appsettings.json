{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DataWarehouse": "Server=(localdb)\\MSSQLLocalDB;Database=SalesAnalyticsDW;Trusted_Connection=True;TrustServerCertificate=True;",
    "SourceDatabase": "Server=(localdb)\\MSSQLLocalDB;Database=SalesSource;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "DataSources": {
    "CsvPath": "Data/orders.csv",
    "CustomersCsvPath": "Data/customers.csv",
    "ProductsCsvPath": "Data/products.csv",
    "OrderDetailsCsvPath": "Data/order_details.csv"
  },
  "ApiSettings": {
    "BaseUrl": "http://localhost:5000",
    "ApiKey": "your-api-key-here",
    "Timeout": 30
  },
  "DimensionJobs": {
    "DimCliente": {
      "Enabled": true,
      "IntervalMinutes": 30,
      "StartupDelaySeconds": 5,
      "BatchSize": 1000
    },
    "DimProducto": {
      "Enabled": true,
      "IntervalMinutes": 30,
      "StartupDelaySeconds": 10,
      "BatchSize": 1000
    },
    "DimTiempo": {
      "Enabled": true,
      "IntervalMinutes": 60,
      "StartupDelaySeconds": 15,
      "BatchSize": 500
    },
    "DimEstado": {
      "Enabled": true,
      "IntervalMinutes": 60,
      "StartupDelaySeconds": 20,
      "BatchSize": 100
    }
  },
  "ETLSettings": {
    "IntervalMinutes": 60,
    "BatchSize": 1000,
    "EnableParallelProcessing": true,
    "LoadDimensionsFirst": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}